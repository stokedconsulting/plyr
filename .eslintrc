{
  "parser": "@babel/eslint-parser",
  "extends": ["@sampotts/eslint-config/es6"],
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Plyr": false,
    "jQuery": false
  },
  "rules": {
    "import/no-cycle": "warn"
  },
  "parserOptions": {
    "sourceType": "module",
    "requireConfigFile": false
  }
}
